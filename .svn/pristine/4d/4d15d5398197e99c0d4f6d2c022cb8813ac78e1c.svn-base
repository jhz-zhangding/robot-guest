<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginRight="@dimen/padding_40"
    android:layout_marginTop="@dimen/padding_15dp"
    android:layout_marginBottom="@dimen/padding_15dp"
    android:background="@color/white"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="180dp"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:gravity="center"
        android:layout_gravity="center_vertical"
        android:text="@string/setting_title"
        android:textSize="@dimen/title_size_30"
        android:focusable="true"
        />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/padding_60"
        android:layout_weight="1"
        android:orientation="vertical"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ul_place_ck2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/CustomCheckBoxTheme"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="上左："
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit2"
                android:layout_width="@dimen/margin_100"
                android:layout_height="wrap_content"
                android:inputType="number" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textSize="@dimen/textsize_26" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/setting_page_view_height"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ul_place_ck6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/CustomCheckBoxTheme"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="下左："
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit6"
                android:layout_width="@dimen/margin_100"
                android:layout_height="wrap_content"
                android:inputType="number" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textSize="@dimen/textsize_26" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ul_place_ck1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                style="@style/CustomCheckBoxTheme"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="上中："
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit1"
                android:layout_width="@dimen/margin_100"
                android:layout_height="wrap_content"
                android:text="100"
                android:inputType="number" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textSize="@dimen/textsize_26" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/setting_page_view_height"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ul_place_ck5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                style="@style/CustomCheckBoxTheme"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="下中："
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit5"
                android:layout_width="@dimen/margin_100"
                android:layout_height="wrap_content"
                android:text="100"
                android:inputType="number" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textSize="@dimen/textsize_26" />
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ul_place_ck3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/CustomCheckBoxTheme"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="上右："
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit3"
                android:layout_width="@dimen/margin_100"
                android:layout_height="wrap_content"
                android:inputType="number" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textSize="@dimen/textsize_26" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/setting_page_view_height"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ul_place_ck4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/CustomCheckBoxTheme"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="下右："
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit4"
                android:layout_width="@dimen/margin_100"
                android:layout_height="wrap_content"
                android:inputType="number" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textSize="@dimen/textsize_26" />
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
