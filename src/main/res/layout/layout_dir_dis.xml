<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0099FF"
    android:gravity="center"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="左"
            android:textColor="@color/white"
            android:textSize="40sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="左1："
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:minWidth="70dp"
                android:text="100"
                android:textColor="@color/guest_text_color" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:text="左1："
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:minWidth="70dp"
                android:text="100"
                android:textColor="@color/guest_text_color" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/setting_page_view_height"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="左2："
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:minWidth="70dp"
                android:textColor="@color/guest_text_color" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:text="左1："
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:minWidth="70dp"
                android:text="100"
                android:textColor="@color/guest_text_color" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0.5"
        android:gravity="center_horizontal"
        android:minWidth="250dp"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="中"
            android:textColor="@color/white"
            android:textSize="40sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="上中："
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:minWidth="70dp"
                android:text="100"
                android:textColor="@color/guest_text_color" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/setting_page_view_height"
            android:gravity="center_horizontal"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="下中："
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:minWidth="70dp"
                android:textColor="@color/guest_text_color" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="右"
            android:textColor="@color/white"
            android:textSize="40sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="上右："
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:minWidth="70dp"
                android:text="100"
                android:textColor="@color/guest_text_color" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:text="左1："
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:minWidth="70dp"
                android:text="100"
                android:textColor="@color/guest_text_color" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/setting_page_view_height"
            android:gravity="center_vertical"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="下右："
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:minWidth="70dp"
                android:textColor="@color/guest_text_color" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:text="左1："
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />

            <EditText
                android:id="@+id/ul_place_edit13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:minWidth="70dp"
                android:text="100"
                android:textColor="@color/guest_text_color" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/distance_unit"
                android:textColor="@color/guest_text_color"
                android:textSize="@dimen/textsize_26" />
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
